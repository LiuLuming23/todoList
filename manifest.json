{
  "name": "TodoList Program",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "一款简单的TodoList待办清单拓展",
  "action": {
    "default_icon": "./icons/icon.png",
    "default_popup": "./popup/index.html"
  },
  "icons": {
    "16": "./icons/icon.png",
    "32": "./icons/icon.png",
    "48": "./icons/icon.png",
    "128": "./icons/icon.png"
  },
    "permissions":
  [
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
